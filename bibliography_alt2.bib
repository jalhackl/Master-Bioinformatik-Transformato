@article{Steinbrecher.2011,
 abstract = {Molecular dynamics-based free energy calculations allow the determination of a variety of thermodynamic quantities from computer simulations of small molecules. Thermodynamic integration (TI) calculations can suffer from instabilities during the creation or annihilation of particles. This {\textquotedbl}singularity{\textquotedbl} problem can be addressed with {\textquotedbl}soft-core{\textquotedbl} potential functions which keep pairwise interaction energies finite for all configurations and provide smooth free energy curves. {\textquotedbl}One-step{\textquotedbl} transformations, in which electrostatic and van der Waals forces are simultaneously modified, can be simpler and less expensive than {\textquotedbl}two-step{\textquotedbl} transformations in which these properties are changed in separate calculations. Here, we study solvation free energies for molecules of different hydrophobicity using both models. We provide recommended values for the two parameters \textgreek{a}(LJ) and \textgreek{b}(C) controlling the behavior of the soft-core Lennard-Jones and Coulomb potentials and compare one- and two-step transformations with regard to their suitability for numerical integration. For many types of transformations, the one-step procedure offers a convenient and accurate approach to free energy estimates.},
 author = {Steinbrecher, Thomas and Joung, InSuk and Case, David A.},
 year = {2011},
 title = {Soft-core potentials in thermodynamic integration: comparing one- and two-step transformations},
 keywords = {Hydrophobic and Hydrophilic Interactions;Methods;Models, Chemical;Models, Molecular;Thermodynamics},
 pages = {3253--3263},
 volume = {32},
